import java.util.Arrays;

public class Main {
    public static void main(String[] args) {

        int[] nums = {1, 3, 5, 6};
        int target = 5;
        int index = Arrays.binarySearch(nums, target);

        if (index >= 0) {
            System.out.println(index);
        } else {
            System.out.println(-index - 1);
        }
    }
}
