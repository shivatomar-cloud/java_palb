   public class Main
{
	public static void main(String[] args) {

		int[]arr= {25,45,48,15,36,19,20};

		int min=arr[0];
		int max=arr[0];

		for(int i:arr) {
			if(i>max) {
				max=i;
			}
			if(i<min) {
				min=i;
			}
		}

		System.out.println("MAXIMUM element: "+max);
		System.out.println("MINIMUM element: "+min);

	}
}
