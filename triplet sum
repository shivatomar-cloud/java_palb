import java.util.*;

public class Main {
	public static void main(String[] args) {
		int[] arr= {1,4,45,6,10,8};
		int target=13;
		Arrays.sort(arr);
		boolean f=false;
		for(int i=0; i<arr.length-2; i++) {
			int l=i+1,r=arr.length-1;
			while(l<r) {
				int s=arr[i]+arr[l]+arr[r];
				if(s==target) {
					f=true;
					break;
				}
				if(s<target)l++;
				else r--;
			}
		}
		System.out.println(f);
	}
}
